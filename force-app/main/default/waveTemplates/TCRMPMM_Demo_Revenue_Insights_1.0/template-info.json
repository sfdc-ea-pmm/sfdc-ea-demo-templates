    "templateBadge": {
      "name": "sales.png"
    },
    "templatePreviews": [
      {
        "name": "preview_revenue_insights1.png",
        "label": "Overview",
        "description": "User the Overview tab to get a comprehensive view of your revenue performance and forecasts."
      },
      {
        "name": "preview_revenue_insights2.png",
        "label": "Team Performance",
        "description": "Use the Team Performance tab to explore your opportunity data and understand pipeline health."
      },
      {
        "name": "preview_revenue_insights3.png",
        "label": "Sales Performance",
        "description": "Use the Sales Performance tab to explore your opportunity data and pipeline health over time."
      }
    ]
  },
