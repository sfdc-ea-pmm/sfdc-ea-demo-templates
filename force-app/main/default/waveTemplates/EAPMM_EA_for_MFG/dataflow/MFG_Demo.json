{
  "isDeletable": true,
  "notificationLevel": "Never",
  "workflowDefinition": {
    "edge_MFG_Demo_Data": {
      "action": "edgemart",
      "parameters": {"alias": "${App.Datasets.MFG_Demo_Data.FullyQualifiedName}"}
    },
    "ComputeDates": {
      "action": "computeExpression",
      "parameters": {
        "source": "Lookup_Account_Id",
        "mergeWithSource": true,
        "computedFields": [
          {
            "name": "TimeAux",
            "saqlExpression": "toDate('Time_sec_epoch')",
            "format": "yyyy-MM-dd",
            "label": "TimeAux",
            "type": "Date",
            "isYearEndFiscalYear": true
          },
          {
            "precision": 16,
            "name": "ElapsedSeconds",
            "saqlExpression": "31536000 * date_diff(\"year\", toDate('LastProcessedDate_sec_epoch'), now())",
            "scale": 0,
            "label": "ElapsedSeconds",
            "type": "Numeric"
          }
        ]
      }
    },
    "ShiftDates": {
      "action": "computeExpression",
      "parameters": {
        "source": "DropDates",
        "mergeWithSource": true,
        "computedFields": [{
          "name": "Time",
          "saqlExpression": "toDate('TimeAux_sec_epoch' + 'ElapsedSeconds')",
          "format": "yyyy-MM-dd",
          "label": "Time",
          "type": "Date",
          "isYearEndFiscalYear": true
        }]
      }
    },
    "digest_Account": {
      "action": "sfdcDigest",
      "parameters": {
        "fields": [
          {"name": "Id"},
          {"name": "Name"},
          {"name": "External_ID__c"}
        ],
        "object": "Account"
      }
    },
    "AddToday": {
      "action": "computeExpression",
      "parameters": {
        "source": "DropAux",
        "mergeWithSource": true,
        "computedFields": [{
          "saqlExpression": "now()",
          "name": "LastProcessedDate",
          "format": "yyyy-MM-dd",
          "label": "LastProcessedDate",
          "type": "Date",
          "isYearEndFiscalYear": true
        }]
      }
    },
    "DropDates": {
      "action": "sliceDataset",
      "parameters": {
        "mode": "drop",
        "source": "ComputeDates",
        "fields": [
          {"name": "Time"},
          {"name": "LastProcessedDate"}
        ]
      }
    },
    "DropAux": {
      "action": "sliceDataset",
      "parameters": {
        "mode": "drop",
        "source": "ShiftDates",
        "fields": [
          {"name": "TimeAux"},
          {"name": "ElapsedSeconds"}
        ]
      }
    },
    "reg_MFG_Demo_Data": {
      "action": "sfdcRegister",
      "parameters": {
        "name": "MFG_Demo_Data",
        "alias": "${App.Datasets.MFG_Demo_Data.FullyQualifiedName}",
        "source": "AddToday"
      }
    },
    "Lookup_Account_Id": {
      "action": "update",
      "parameters": {
        "right_key": ["External_ID__c"],
        "left": "edge_MFG_Demo_Data",
        "left_key": ["ExternalId"],
        "update_columns": {"AccountId": "Id"},
        "right": "digest_Account"
      }
    }
  },
  "DataflowType": "USER",
  "_type": "dataflow",
  "name": "MFG_Demo",
  "MasterLabel": "MFG Demo",
  "_url": "/insights/internal_api/v1.0/esObject/workflow//json",
  "_uid": "",
  "nextRun": "",
  "WorkflowStatus": "Inactive"
}